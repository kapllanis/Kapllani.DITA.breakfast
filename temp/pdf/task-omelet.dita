            <step class="- topic/li task/step " xtrc="step:2;35:19" xtrf="file:/C:/Users/Stephanie/Documents/GitHub/Kapllani.DITA.breakfast/task-omelet.dita">
                <cmd class="- topic/ph task/cmd " xtrc="cmd:2;36:22" xtrf="file:/C:/Users/Stephanie/Documents/GitHub/Kapllani.DITA.breakfast/task-omelet.dita">Add salt to the eggs and blend with a fork.</cmd>
            </step>
            <step class="- topic/li task/step " xtrc="step:3;38:19" xtrf="file:/C:/Users/Stephanie/Documents/GitHub/Kapllani.DITA.breakfast/task-omelet.dita">
                <cmd class="- topic/ph task/cmd " xtrc="cmd:3;39:22" xtrf="file:/C:/Users/Stephanie/Documents/GitHub/Kapllani.DITA.breakfast/task-omelet.dita">Heat a pan over medium-high heat.</cmd>
            </step>
